---
import { clientOauth } from "@/firebase/config";
const web = clientOauth.web;

const { client_id, redirect_uris } = web;
const authUrl = `https://accounts.google.com/o/oauth2/v2/auth?client_id=${client_id}&redirect_uri=${redirect_uris[0]}&response_type=code&scope=email openid`;
return Astro.redirect(authUrl);
---
